pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Build'
                sleep 10
            }
        }
        stage('Q/A') {
            steps {
                echo 'Q/A'
                sleep 10
            }
        }
        stage('Publish') {
            steps {
                echo 'Publish'
                sleep 10
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
                sleep 10
            }
        }
    }
}